<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_edit_product_spec"
    app:startDestination="@id/provideCouponFragment">

    <fragment
        android:id="@+id/provideCouponFragment"
        android:name="com.same.part.assistant.fragment.ProvideCouponFragment"
        android:label="ProvideCouponFragment">
        <action
            android:id="@+id/action_provideCouponFragment_to_selectCustomFragment"
            app:destination="@id/selectCustomFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>

    <fragment
        android:id="@+id/selectCustomFragment"
        android:name="com.same.part.assistant.fragment.SelectCustomFragment"
        android:label="SelectCustomFragment" >
    </fragment>
</navigation>